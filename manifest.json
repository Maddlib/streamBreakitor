{
    "manifest_version": 2,
    "name": "StreamBreakitor",
    "version": "0.4",
    "description": "Breaks up streams on monitor",
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
      },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "background": {
        "scripts": ["click.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Unstream this page."
    },
    "icons":{
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}